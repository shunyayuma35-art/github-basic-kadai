        "# Humanクラスの定義\n",
        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name  # 名前の属性\n",
        "        self.age = age    # 年齢の属性\n",
        "\n",
        "    # 名前と年齢を出力するメソッド\n",
        "    def printinfo(self):\n",
        "        print(\"名前：\", self.name)\n",
        "        print(\"年齢：\", self.age)\n",
        "\n",
        "# Humanクラスのインスタンスを作成して変数に代入\n",
        "person = Human(\"太郎\", 25)\n",
        "\n",
        "# メソッドを使用して名前と年齢を出力\n",
        "person.printinfo()"
