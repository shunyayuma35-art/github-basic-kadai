        "# Step1. ライブラリのインポート\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import japanize_matplotlib\n",
        "\n",
        "\n",
        "# ▼ 前提：ここで df が作成済みであること\n",
        "# df = pd.merge(df, category_df[['商品番号', 'カテゴリー']], how='inner', on='商品番号')\n",
        "# ※教材7.4章で作成した df がそのまま使われる\n",
        "\n",
        "\n",
        "# Step2. カテゴリー列の出現頻度をカウント → 棒グラフ表示\n",
        "category_counts = df['カテゴリー'].value_counts()\n",
        "\n",
        "category_counts.plot(kind='bar', figsize=(6,4))\n",
        "plt.title(\"カテゴリー別の出現頻度\")\n",
        "plt.xlabel(\"カテゴリー\")\n",
        "plt.ylabel(\"件数\")\n",
        "\n",
        "# Step3. グラフを表示\n",
        "plt.show()\n",
        "\n",
        "\n",
        "# Step4. 商品番号ごとに注文数の基本統計量を算出\n",
        "# （注文数の列名が教材通り「注文数」である前提）\n",
        "stats = df.groupby('商品番号')['注文数'].describe()\n",
        "\n",
        "# 結果表示\n",
        "print(\"商品番号ごとの基本統計量：\")\n",
        "print(stats)"
