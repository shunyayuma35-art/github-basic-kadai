        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(self.name)\n",
        "        print(self.age)\n",
        "\n",
        "# インスタンス作成\n",
        "person = Human(\"Taro\", 25)\n",
        "\n",
        "# 出力\n",
        "person.printinfo()"
