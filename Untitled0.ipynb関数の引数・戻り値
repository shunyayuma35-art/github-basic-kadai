      "cell_type": "code",
      "source": [
        "def greet_user(user_name: str) -> str:\n",
        "    if user_name == \"\":\n",
        "        return \"こんにちは、ゲストさん！\"\n",
        "    else:\n",
        "        return f\"こんにちは、{user_name}さん！\"\n",
        "\n",
        "# 実行例\n",
        "print(greet_user(\"太郎\"))   # こんにちは、太郎さん！\n",
        "print(greet_user(\"\"))      # こんにちは、ゲストさん！"
      ],
