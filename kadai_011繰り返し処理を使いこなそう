        "# リストの定義\n",
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "# --- for文でリストの中身を出力 ---\n",
        "for planet in array:\n",
        "    print(planet)\n",
        "\n",
        "# --- while文でリストの中身を出力 ---\n",
        "i = 0\n",
        "while i < len(array):\n",
        "    print(array[i])\n",
        "    i += 1"
