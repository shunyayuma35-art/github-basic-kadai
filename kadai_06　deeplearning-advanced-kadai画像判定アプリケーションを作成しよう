        "<!-- prediction/templates/prediction/index.html -->\n",
        "<!DOCTYPE html>\n",
        "<html>\n",
        "<head>\n",
        "    <title>VGG16画像判定アプリ</title>\n",
        "</head>\n",
        "<body>\n",
        "<h1>画像判定アプリ</h1>\n",
        "\n",
        "<form method=\"post\" enctype=\"multipart/form-data\">\n",
        "    {% csrf_token %}\n",
        "    {{ form.as_p }}\n",
        "    <button type=\"submit\">判定</button>\n",
        "</form>\n",
        "\n",
        "{% if img_url %}\n",
        "<h2>アップロード画像</h2>\n",
        "<img src=\"{{ img_url.url }}\" alt=\"uploaded image\" width=\"200\">\n",
        "{% endif %}\n",
        "\n",
        "{% if result %}\n",
        "<h2>判定結果（上位5カテゴリ）</h2>\n",
        "<table border=\"1\">\n",
        "<tr>\n",
        "    <th>ID</th><th>カテゴリ名</th><th>確率</th>\n",
        "</tr>\n",
        "{% for r in result %}\n",
        "<tr>\n",
        "    <td>{{ r.0 }}</td>\n",
        "    <td>{{ r.1 }}</td>\n",
        "    <td>{{ r.2 }}</td>\n",
        "</tr>\n",
        "{% endfor %}\n",
        "</table>\n",
        "{% endif %}\n",
        "\n",
        "</body>\n",
        "</html>"
