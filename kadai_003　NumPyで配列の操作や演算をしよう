        "# Step1. NumPyをインポート\n",
        "import numpy as np\n",
        "\n",
        "# Step2. 行列Aと行列Bを多次元配列で作成\n",
        "# 例として課題でよく使われる行列\n",
        "# A = [[1, 2, 3],\n",
        "#      [4, 5, 6]]\n",
        "#\n",
        "# B = [[7,  8],\n",
        "#      [9, 10],\n",
        "#      [11,12]]\n",
        "\n",
        "A = np.array([[1, 2, 3],\n",
        "              [4, 5, 6]])\n",
        "\n",
        "B = np.array([[7,  8],\n",
        "              [9, 10],\n",
        "              [11,12]])\n",
        "\n",
        "# Step3. 行列の積を計算\n",
        "C = np.dot(A, B)\n",
        "\n",
        "# Step4. 行列の積の結果を表示\n",
        "print(\"行列A × 行列B の結果：\")\n",
        "print(C)\n",
        "\n",
        "# Step5. 計算結果Cの最大値を求めて表示\n",
        "max_value = np.max(C)\n",
        "print(\"計算結果の最大値：\", max_value)"
      ],
